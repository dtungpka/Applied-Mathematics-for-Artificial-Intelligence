    "with open(\"./doc.txt\",'r') as f:\n",
    "    words_counts = {}\n",
    "    for word in f.read().replace(\'.\',\'\').split():\n",
    "        word = word.lower()\n",
    "        if word in words_counts:\n",
    "            words_counts[word] += 1\n",
    "        else:\n",
    "            words_counts[word] = 1\n",
    "    for k,v in words_counts.items():\n",
    "        print(\"%-12s|\" % k,v)"
